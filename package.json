{
  "name": "@quantaris/sdk",
  "version": "0.1.0",
  "description": "Client SDK and bot toolkit for the Quantaris deterministic strategy game",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@quantaris/engine": "file:../quantaris-engine"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "quantaris",
    "sdk",
    "client",
    "bot",
    "strategy",
    "game"
  ],
  "author": "Quantaris Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantaris-live/quantaris-sdk.git"
  }
}

